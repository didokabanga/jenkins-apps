node{

  stage('SCM Checkout'){
    git branch: 'main', url: 'https://github.com/didokabanga/jenkins-apps.git'
  }

  stage('Compile-Package'){
    // Get Maven Home Path
    def mvnHome =  tool name: 'MY_MAVEN3', type: 'maven'
 
    dir('jenkins-webapp01/') {
         // some block
         sh "${mvnHome}/bin/mvn package"
    }
  }

}
